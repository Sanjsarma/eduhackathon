  <div class="row" id="main" style = "    margin: 2% 2% 2% 2%;    ">
              
    <div class="container">
        <div class="col-sm-12 col-md-12 well" id="content" style="    background-color: #f5f5f56b; margin-top: 1%;    color: #080808;" >
            <h1 style="text-align: center;"><strong>Welcome to the Materials  Section!</strong></h1>
        </div>
       
        <div class="row" style = "    background-color:white;">
            <div class="col-md-10 col-md-offset-1">
    
                <div class="panel panel-default panel-table" style = "font-size: 1.5rem;">
                  <div class="panel-heading" style = "background-color:black;
                  color: white;">
                    <div class="row">
                      <div class="col col-xs-6">
                        <h3 class="panel-title" style = "    font-size: 1.75rem;
                        margin-top: 1.25%;    "><strong> Video Materials</strong></h3>
                      </div>
                      <div class="col col-xs-6 text-right">
                      
                      </div>
                    </div>
                  </div>
                  <div class="panel-body">
                    <table class="table table-striped table-bordered table-list">
                      <thead>
                        <tr>
                            <th>Course ID</th>
                            <th>Module No</th>
                            <th>Video Link</th>
                        
                        </tr> 
                      </thead>
                      <tbody>
                        <%
                        if(userData.length!=0){
                        var i=1;
                        userData.forEach(function(data){
                        %>
                        <tr>
                            <td><%=data.courseid %></td>
                            <td><%=data.mod_no %></td>
                            <td><%=data.video_link%></td>
                           
                            
                        </tr>
                        <%  i++; }) %>
                        <% } else{ %>
                            <tr>
                                <td colspan="7">No Data Found</td>
                            </tr>
                        <% } %>
                            </tbody>
                    </table>
                
                  </div>
                 
    
    </div></div></div>
  
